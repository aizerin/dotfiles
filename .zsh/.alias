alias y="yarn"
alias ys="yarn start"
alias yr="yarn run"
alias yt="yarn test"

alias n="npm"
alias ni="npm i"
alias nr="npm run"

alias ga="git commit --amend --no-edit"
alias gf="git push --force"
alias gaf="ga && gf"
alias gaaf="git add . && gaf"
alias gfo="git fetch origin"

killPort() { lsof -nP -t -i4TCP:"$1" | xargs kill -9 }
restartAudio() { sudo pkill coreaudiod }

noSleep() { pmset noidle }
